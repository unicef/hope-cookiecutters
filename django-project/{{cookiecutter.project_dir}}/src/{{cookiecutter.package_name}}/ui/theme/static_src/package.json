{
  "name": "theme",
  "version": "4.2.0",
  "description": "",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run build:clean && npm run build:tailwind",
    "build:clean": "rimraf ../static/css/*",
    "build:tailwind": "cross-env NODE_ENV=production postcss src/styles.scss -o ../static/css/styles.css --minify",
    "dev": "cross-env NODE_ENV=development postcss src/styles.scss -o ../static/css/styles.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1",
    "cross-env": "^7.0.3",
    "daisyui": "^5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "4.0.0-alpha.17"
  }
}
